{
  "name": "supervisor",
  "version": "1.0.0",
  "description": "TypeScript Node.js API with Express",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "test": "ts-node src/test.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "build-docker": "docker build -t test .",
    "chat": "ts-node src/chat.ts",
    "cronDev": "ts-node src/cron.ts",
    "cron": "node dist/cron.js",
    "migrate": "ts-node src/migration-cli.ts migrate",
    "migrate:rollback": "ts-node src/migration-cli.ts rollback",
    "migrate:status": "ts-node src/migration-cli.ts status",
    "create:migration": "ts-node src/create-migration.ts",
    "zip": "rm -rf release.zip && zip -r release.zip Dockerfile ecosystem.json greeting.txt package.json tsconfig.json env_example src -x src/chat.ts -x src/test.ts",
    "unzip": "rm -rf release && unzip release.zip -d release",
    "podmanbuild": "yarn zip && yarn unzip && cd ./release && podman build -t localhost/84532_agent_9518 .",
    "podmanrun": "podman run --env-file .env -p 3000:80 -p 6080:6080 -it localhost/84532_agent_9518"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "agent-server-definition": "^0.2.20",
    "ai": "^4.3.17",
    "async-mutex": "^0.5.0",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "cron": "^4.3.1",
    "discord.js": "^14.21.0",
    "dotenv": "^16.4.5",
    "ethers": "5.7.2",
    "express": "^4.21.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "node-cron": "^4.1.1",
    "playwright": "=1.54.1",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7",
    "winston": "^3.11.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
